{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [

    
    {
      "type": "node",
      "request": "launch",
      "name": "server",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "./server-src/server.js",
      "cwd": "${workspaceFolder}",
    },
    {
      "type": "node",
      "request": "launch",
      "name": "api",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "./api-src/api.js",
      "cwd": "${workspaceFolder}",
    },
    {
      "name": ".NET Core Launch (WPF)",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}/NoteEditorApp/bin/Debug/net7.0-windows/NoteEditorApp.exe",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "console": "internalConsole",
      "logging": {
        "moduleLoad": false
      }
    }
  ]
}